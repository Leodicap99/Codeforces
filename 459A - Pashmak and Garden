    #include <bits/stdc++.h>
    typedef long long ll;
    using namespace std;
    int main() {
        ios::sync_with_stdio(0);
        cin.tie(0);
        ll x1,y1,x2,y2;
        cin>>x1>>y1>>x2>>y2;
        if(abs(x1-x2)==abs(y1-y2))
        cout<<x1<<" "<<y2<<" "<<x2<<" "<<y1;
        else if(x1==x2)cout<<x1+y2-y1<<" "<<y1<<" "<<x2+y2-y1<<" "<<y2;
        else if(y1==y2)cout<<x1<<" "<<y1+x2-x1<<" "<<x2<<" "<<y1+x2-x1;
        else cout<<"-1";
        return 0;
    }
