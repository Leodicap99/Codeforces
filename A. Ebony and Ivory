        #include <bits/stdc++.h>
        using namespace std;
        typedef long long ll;
        int main ()
        {
            ios::sync_with_stdio(0);
            cin.tie(0);
            int a,b,c;
            cin>>a>>b>>c;
            while(c>=0)
            {
                if(c%b==0)
                {
                    cout<<"YES";
                    return 0;
                }
                c-=a;
            }
            cout<<"NO";
        	return 0;
        }
